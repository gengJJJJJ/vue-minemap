{
	"name": "v-minemap",
	"version": "1.0.4",
	"private": false,
	"author": {
		"name": "GengJJJJJ"
	},
	"description": "Vue2 地图组件库，基于四维地图minemap SDK 封装",
	"license": "MIT",
	"main": "dist/v-minemap.umd.js",
	"module": "dist/v-minemap.common.js",
	"unpkg": "dist/v-minemap.umd.min.js",
	"sideEffects": false,
	"files": [
		"dist"
	],
	"keywords": [
		"vue2",
		"minemap",
		"地图",
		"四维地图",
		"vue-minemap"
	],
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build --target lib --name v-minemap packages/index.js && node scripts/copySource.js",
		"lint": "npm run lint-eslint",
		"lint-eslint": "eslint . --fix",
		"format": "prettier --write src/",
		"postinstall": "npx simple-git-hooks"
	},
	"dependencies": {
		"vue": "^2.6.14",
		"coordtransform": "^2.1.2"
	},
	"devDependencies": {
		"@babel/core": "^7.12.16",
		"@babel/eslint-parser": "^7.12.16",
		"@commitlint/cli": "^19.8.0",
		"@commitlint/config-conventional": "^19.8.0",
		"@vue/cli-plugin-babel": "~5.0.0",
		"@vue/cli-plugin-eslint": "~5.0.0",
		"@vue/cli-service": "~5.0.0",
		"axios": "^0.27.2",
		"babel-eslint": "^10.1.0",
		"babel-plugin-component": "^1.1.1",
		"core-js": "^3.8.3",
		"element-ui": "^2.15.5",
		"eslint": "^7.32.0",
		"eslint-plugin-vue": "^8.0.3",
		"fs-extra": "^11.3.0",
		"lint-staged": "^15.5.1",
		"mitt": "^3.0.1",
		"prettier": "^3.5.3",
		"sass": "^1.26.5",
		"sass-loader": "^8.0.2",
		"simple-git-hooks": "^2.12.1",
		"vue-router": "^3.6.5",
		"vue-template-compiler": "^2.6.14",
		"vuex": "^3.6.2"
	},
	"simple-git-hooks": {
		"commit-msg": "npx --no-install commitlint --edit $1",
		"pre-commit": "npx lint-staged"
	},
	"lint-staged": {
		"*.js": "eslint --fix",
		"*.vue": "eslint --fix"
	},
	"commitlint": {
		"extends": "@commitlint/config-conventional"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"parserOptions": {
			"parser": "@babel/eslint-parser"
		},
		"rules": {}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	]
}
